{
    "name": "phpbenchmarks/symlex",
    "license": "MIT",
    "type": "project",
    "description": "Benchmarks for Symlex",
    "homepage": "https://github.com/phpbenchmarks/symlex",
    "authors": [
        {
            "name": "Michael Mayer",
            "email": "michael@liquidbytes.net",
            "homepage": "https://liquidbytes.net/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-pdo_mysql": "*",
        "symlex/symlex-core": "^4.1",
        "symlex/input-validation": "^4.2",
        "symlex/doctrine-active-record": "^4.3",
        "doctrine/migrations": "^1.1",
        "monolog/monolog": "^1.0",
        "swiftmailer/swiftmailer": "^5.0",
        "incenteev/composer-parameter-handler": "^2.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
      "bin-dir": "bin/"
    },
    "scripts": {
        "post-composer-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "app/clearcache",
            "composer dump-autoload -o -a"
        ],
        "post-install-cmd": [
            "@post-composer-scripts"
        ],
        "post-update-cmd": [
            "@post-composer-scripts"
        ]
    },
    "extra": {
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "dist-file": "app/config/parameters.dist.yml"
        }
    }
}
